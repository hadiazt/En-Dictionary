  <link rel="stylesheet" href="/css/main.css">
  </link>


  <script src="https://kit.fontawesome.com/31fd7c4de0.js" crossorigin="anonymous"></script>

  <link rel="icon" href="<%- icon %>">
  </link>

</head>

<body>

  <% if(!WORD) { %>
    <nav class="navbar">
      <a href="/"><img class="name-img" src="<%- icon %>"></a>
      <form action="/" method="GET">
        <input name="word" id="search" class="search sans" type="text" placeholder="Please Enter The Word To Search">
      </form>
    </nav>
  
    <div class="alarm aviny">
      Welcome To <%- pageTitle %></br>For Start Using, Type The Word In <a class="alarm_a" href="#search">Input</a> Section
    </div>
  <% } else if (WORD && OUTPUT && ERR === ' ') { %>

    <nav class="navbar">
      <a href="/"><img class="name-img" src="<%- icon %>"></a>
      <form action="/" method="GET">
        <input name="ID" class="search sans" type="text" placeholder="Please Enter The Word To Search">
      </form>
    </nav>
    WORD && OUTPUT && !ERR
  <% } else if (WORD && OUTPUT && ERR[2]) { %>
    WORD && OUTPUT && ERR
    <%- ERR %>

  <% } %>




  <div class="footer sans">
    <h4>
      Created By <a class="fotter_a" href="https://hadi-az.me" target="_blank">Hadi.az</a> | <a class="fotter_a" href="https://github.com/hadiazt/En-Dictionary" target="_blank">Source</a>
      </br>
    </h4>
  </div>

</body>



</html>